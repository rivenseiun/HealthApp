<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Circle">
        <attr name="circleBackground" format="integer" />
        <attr name="firstChildPosition">
            <enum name="East" value="0" />
            <enum name="South" value="90" />
            <enum name="West" value="180" />
            <enum name="North" value="270" />
        </attr>
        <attr name="rotateToCenter" format="boolean" />
        <attr name="isRotating" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="name" format="string" />
    </declare-styleable>
    <declare-styleable name="ChangeIconColorWithText">
        <attr name="icon" />
        <attr name="text" format="string" />
        <attr name="text_size" format="dimension" />
        <attr name="color" format="color" />
    </declare-styleable>
    
    
    <declare-styleable name="CircleProgressBar">
        <attr name="progressbar_width" format="dimension" />
        <attr name="progress_background_color" format="color" />
        <attr name="progress_color" format="color" />
    </declare-styleable>

</resources>